{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "outDir": "dist",
    "target": "es6",
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": "./src/",
    "paths": {
      "@utils/*": ["./utils/*"]
    },
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}
